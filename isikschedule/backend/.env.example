# Application
APP_NAME=IşıkSchedule
APP_ENV=development
DEBUG=true
SECRET_KEY=your-secret-key-change-in-production

# Database
DATABASE_URL=postgresql+asyncpg://postgres:postgres@localhost:5432/isikschedule

# Redis
REDIS_URL=redis://localhost:6379/0

# File Storage
UPLOAD_DIR=./uploads
MAX_FILE_SIZE_MB=10

# CORS
CORS_ORIGINS=http://localhost:3000,http://127.0.0.1:3000

# Rate Limiting
RATE_LIMIT_PER_MINUTE=100

# Job Settings
JOB_TIMEOUT_SECONDS=300
MAX_SCHEDULES_PER_JOB=50
